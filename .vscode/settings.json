    "editorCursor.foreground": "#77a3c4",
    "inputValidation.errorBorder": "#77a3c4",
    "panel.border": "#77a3c4",
    "panelTitle.activeBorder": "#77a3c4",
    "panelTitle.activeForeground": "#77a3c4",
    "peekView.border": "#77a3c4",
    "peekViewTitleLabel.foreground": "#77a3c4",
